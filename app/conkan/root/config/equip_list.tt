<!DOCTYPE html>
<html lang="ja">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="description" content="conkan equiplist">
<meta name="author" content="Studio REM">
<title>Conkan EquipList</title>
[%- SET self_li_id = 'config_equip' -%]
[%- SET useUiGrid  = 1 -%]
[%- PROCESS 'tthead.tt' -%]
<div class="container" ng-app="conkanEquipList">
  <div class="well" id="gridlist" ng-controller="equipListController">
    <div id="equiplist" ui-grid="equipgrid" class="grid" ui-grid-resize-columns>
    </div>
  </div> <!-- End of well gridlist -->
  <div id="pgAddDiv">
    <button type="button" class="btn btn-primary"
            data-toggle="modal" data-target="#editEquip"
            data-whatever="0" data-rmcol="">
      追加
    </button>
  </div>
  [%- PROCESS 'T_httpget_result.tt' -%]
</div> <!-- End of container ng-app "conkanEquipList" -->
<!-- 機材詳細モーダル -->
<div class="modal fade" id="editEquip" tabindex="-1" role="dialog" aria-labelledby="editEquipLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="editEquipLabel">Edit EquipInfo</h4>
      </div>
      <div class="modal-body" id="editEquipContent">
        <FORM>情報取得中</FORM>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">閉じる</button>
        <button type="button" class="btn " id="dodel">無効化</button>
        <button type="button" class="btn btn-primary" id="dobtn">登録/更新</button>
      </div>
    </div>
  </div>
</div>
[%- PROCESS 'tttail.tt' -%]
<script type="text/javascript" src="/static/js/conkan_equip_list.js"></script>
</body>
</html>
